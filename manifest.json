{
	"manifest_version": 2,
	"name": "GoogEvening",
	"version": "1.1",

	"description": "Adds a dark background to google search site",

	"icons": {
		"48": "icons/moon.png"
	},

	"content_scripts": [
		{
			"matches": ["*://www.google.com/search*", "*://www.google.com/"],
			"js": ["GoogEvening.js"]
		}
	]
}
